@model Harmonogram

<form method="post" enctype="multipart/form-data">
    <div class="border p-3 mt-4">
        <div class="row pb-2">
            <h2 class="text-primary">Utwórz nowy harmonogram</h2>
            <hr />
        </div>
        <div class="mb-3">
            <label asp-for="OdKiedy"></label>
            <input type="date" asp-for="OdKiedy" class="form-control" />
            <span asp-validation-for="OdKiedy" class="text-danger"></span>
        </div>
        <div>
            <label asp-for="ListaGodzin"></label>
            <input type="file" name="file" />
            <span asp-validation-for="ListaGodzin" class="text-danger"></span>
        </div>
        <button type="submit" class="btn btn-primary" style="width:150px">Zapisz</button>
        <a asp-controller="Kierownik" asp-action="ListaHarmonogram" class="btn btn-secondary" style="width:150px">
            Wróć
        </a>
    </div>
</form>

@section Scripts{
    @{
        <partial name="_ValidationScriptsPartial" />
    }
}
